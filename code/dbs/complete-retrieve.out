<form method="POST" action="{{!}}">
    <table>
                <tr>
            <td>菜名：</td>
            <td><input name='dishName' type='text' /></td>
        </tr>
        <tr>
            <td>最低價格：</td>
            <td><input name='minPrice' type='text' value='5.00' /></td>
        </tr>
        <tr>
            <td>最高價格：</td>
            <td><input name='maxPrice' type='text' value='25.00' /></td>
        </tr>
        <tr>
            <td>辣：</td>
            <td><select name='isSpicy'><option value='0'>不</option><option value='1'>是</option><option value='2'>兩種</option></select>            </td>
        </tr>
        <tr>
            <td colspan="2" align="center">
                <input name='search' value='搜尋' type='submit' /></td>
        </tr>
    </table>
</form>
